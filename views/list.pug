    link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    link(rel='stylesheet', href='../css/style.min.css')
  body
    .note-template(identification= id)
      div.note-txt
        a(href='/') back
        a(href='/') edit
        a(href='/') delete
      h2.note-title= title
      p.note-txt= content
      p.note-date= date

